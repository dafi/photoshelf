{
    "version":2,
    "blackList":{
            "regExprs":[
              ["([a-z])- ", "$1 - ", "add a whitespace before dash"],
              ["(?i)(january|february|march|april|may|june|july|august|september|october|november|december) (\\d+)(?:st|nd|rd|th|-)\\s?(\\d{4})", ", $1 $2 $3", "superiorpics: convert January 3-2015 to , January 3 2015"],
              ["\\s(\\d).(\\d+).(\\d+) - SuperiorPics Celebrity Gallery", " 0$2/$1/$3", "convert m/dd/yyyy to dd/mm/yyyy and zero fill"],
              ["\\s0(\\d{2}).(\\d+).(\\d+)", " $1/$2/$3", "remove the extra 0. This rules is a workaround for the previous one when the source 1/09/16 is replaced with 009/1/16"],
              ["\\s(\\d+).(\\d+).(\\d+) - SuperiorPics Celebrity Gallery",  " $2/$1/$3", "convert mm/dd/yyyy to dd/mm/yyyy"],
              ["([0-9]+)/([0-9]+)/([0-9]+)\\s?-?\\s?\\bHawtCelebs\\b", "$2/$1/$3", "convert mm/dd/yyyy to dd/mm/yyyy"],
              ["-?\\s?\\bHawtCelebs\\b", "", "remove the text"],
              ["-?\\s?Carreck Celebrity Pictures » Blog Archive »", "", "remove the text"],
              ["-?\\s?SAWFIRST.*", "", "remove the text"]
            ]
    }
}
