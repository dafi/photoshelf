<resources xmlns:tools="http://schemas.android.com/tools">
    <string name="app_name">post-ui-core</string>
    <string name="select_posts">Select Posts</string>
    <string name="post_count_1_of_x" tools:ignore="PluralsCandidate">%1$d/%2$d posts</string>
    <string name="uploaded_at_time">Uploaded at %1$s</string>
    <string name="menu_header_show_image">Image for %1s</string>
